NODE_OPTIONS="--no-deprecation --no-experimental-strip-types"

# 포트원 v2 (portone.io 콘솔에서 복사)
# 콘솔 → 결제연동 → 상점 아이디 (storeId)
NEXT_PUBLIC_PORTONE_STORE_ID=store-xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# 콘솔 → 결제연동 → 채널관리 → 채널키
